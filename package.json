{
  "name": "bulkie.js",
  "version": "0.0.2",
  "main": "./dist/cjs/bulkie.js",
  "module": "./dist/esm/bulkie.js",
  "types": "./dist/types/bulkie.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/bulkie.js",
      "import": "./dist/esm/bulkie.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "bun run dev.ts",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types --project tsconfig.types.json",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@lit-protocol/constants": "^6.9.0",
    "@lit-protocol/contracts-sdk": "^6.9.0",
    "@lit-protocol/lit-node-client": "^6.9.0",
    "@lit-protocol/types": "^6.9.0",
    "ethers": "5.7.2"
  },
  "engines": {
    "bun": ">=0.6.0"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0"
  }
}
